label = 'Individual Level';
fontsize = 28;
color = grey;
gamma_dk [label = 'γ_dk\nAdjust Topic Weights', fillcolor=white];
rho_dk [label = 'ρ_dk\nAdjust Disease Timing', fillcolor=white];
theta_dk [label = 'θ_{dk}(t)\nIndividual Topic Weights', fillcolor=white];
warped_time [label = 'Warped Time (t^{p})', fillcolor=white];
beta_dz [label = 'β_{dz}(t)\nIndividual Disease Probabilities', fillcolor=white];
z_d [label = 'z_{dnt}\nTopic Assignment', fillcolor=white];
w_d [label = 'w_{dnt}\nObserved Data', fillcolor = lightgrey];
gamma_dk -> theta_dk [label = 'Adjust Topic Weights'];
rho_dk -> beta_dz [label = 'Dependency'];
